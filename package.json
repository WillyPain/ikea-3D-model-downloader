{
  "name": "ikea-3d-model-downloader",
  "version": "0.0.1",
  "description": "Takes a link to an Ikea furniture listing and downloads the glb + decompresses the draco compression",
  "main": "dist/app.js",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "build": "tsc && cp src/form.html dist/form.html",
    "dev": "cp src/form.html dist/form.html && concurrently \"tsc --watch\" \"nodemon dist/app.js\""
  },
  "author": "WillyPain",
  "dependencies": {
    "@gltf-transform/cli": "^4.2.0",
    "@gltf-transform/extensions": "^4.2.0",
    "express": "^5.1.0",
    "glob": "^7.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/express": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0"
  }
}
